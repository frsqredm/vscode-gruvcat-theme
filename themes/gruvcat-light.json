{
  "$schema": "vscode://schemas/color-theme",
  "name": "Gruvbox Light Medium",
  "semanticHighlighting": true,
  "tokenColors": [
    {
      "settings": {
        "foreground": "#654735"
      }
    },
    {
      "scope": "emphasis",
      "settings": {
        "fontStyle": "italic"
      }
    },
    {
      "scope": "strong",
      "settings": {
        "fontStyle": "bold"
      }
    },
    {
      "scope": "header",
      "settings": {
        "foreground": "#45707a"
      }
    },
    {
      "scope": ["comment", "punctuation.definition.comment"],
      "settings": {
        "foreground": "#928374",
        "fontStyle": "italic"
      }
    },
    {
      "scope": ["constant", "support.constant", "variable.arguments"],
      "settings": {
        "foreground": "#945e80"
      }
    },
    {
      "scope": "constant.rgb-value",
      "settings": {
        "foreground": "#654735"
      }
    },
    {
      "scope": "entity.name.selector",
      "settings": {
        "foreground": "#4c7a5d"
      }
    },
    {
      "scope": "entity.other.attribute-name",
      "settings": {
        "foreground": "#b47109"
      }
    },
    {
      "scope": ["entity.name.tag", "punctuation.tag"],
      "settings": {
        "foreground": "#4c7a5d"
      }
    },
    {
      "scope": ["invalid", "invalid.illegal"],
      "settings": {
        "foreground": "#c14a4a"
      }
    },
    {
      "scope": "invalid.deprecated",
      "settings": {
        "foreground": "#945e80"
      }
    },
    {
      "scope": "meta.selector",
      "settings": {
        "foreground": "#4c7a5d"
      }
    },
    {
      "scope": "meta.preprocessor",
      "settings": {
        "foreground": "#c35e0a"
      }
    },
    {
      "scope": "meta.preprocessor.string",
      "settings": {
        "foreground": "#6c782e"
      }
    },
    {
      "scope": "meta.preprocessor.numeric",
      "settings": {
        "foreground": "#6c782e"
      }
    },
    {
      "scope": "meta.header.diff",
      "settings": {
        "foreground": "#c35e0a"
      }
    },
    {
      "scope": "storage",
      "settings": {
        "foreground": "#c14a4a"
      }
    },
    {
      "scope": ["storage.type", "storage.modifier"],
      "settings": {
        "foreground": "#c35e0a"
      }
    },
    {
      "scope": "string",
      "settings": {
        "foreground": "#6c782e"
      }
    },
    {
      "scope": "string.tag",
      "settings": {
        "foreground": "#6c782e"
      }
    },
    {
      "scope": "string.value",
      "settings": {
        "foreground": "#6c782e"
      }
    },
    {
      "scope": "string.regexp",
      "settings": {
        "foreground": "#c35e0a"
      }
    },
    {
      "scope": "string.escape",
      "settings": {
        "foreground": "#c14a4a"
      }
    },
    {
      "scope": "string.quasi",
      "settings": {
        "foreground": "#4c7a5d"
      }
    },
    {
      "scope": "string.entity",
      "settings": {
        "foreground": "#6c782e"
      }
    },
    {
      "scope": "object",
      "settings": {
        "foreground": "#654735"
      }
    },
    {
      "scope": "module.node",
      "settings": {
        "foreground": "#45707a"
      }
    },
    {
      "scope": "support.type.property-name",
      "settings": {
        "foreground": "#4c7a5d"
      }
    },
    {
      "scope": "keyword",
      "settings": {
        "foreground": "#c14a4a"
      }
    },
    {
      "scope": "keyword.control",
      "settings": {
        "foreground": "#c14a4a"
      }
    },
    {
      "scope": "keyword.control.module",
      "settings": {
        "foreground": "#4c7a5d"
      }
    },
    {
      "scope": "keyword.control.less",
      "settings": {
        "foreground": "#b47109"
      }
    },
    {
      "scope": "keyword.operator",
      "settings": {
        "foreground": "#4c7a5d"
      }
    },
    {
      "scope": "keyword.operator.new",
      "settings": {
        "foreground": "#c35e0a"
      }
    },
    {
      "scope": "keyword.other.unit",
      "settings": {
        "foreground": "#6c782e"
      }
    },
    {
      "scope": "metatag.php",
      "settings": {
        "foreground": "#c35e0a"
      }
    },
    {
      "scope": "support.function.git-rebase",
      "settings": {
        "foreground": "#4c7a5d"
      }
    },
    {
      "scope": "constant.sha.git-rebase",
      "settings": {
        "foreground": "#6c782e"
      }
    },
    {
      "name": "Types declaration and references",
      "scope": [
        "meta.type.name",
        "meta.return.type",
        "meta.return-type",
        "meta.cast",
        "meta.type.annotation",
        "support.type",
        "storage.type.cs",
        "variable.class"
      ],
      "settings": {
        "foreground": "#b47109"
      }
    },
    {
      "scope": ["variable.this", "support.variable"],
      "settings": {
        "foreground": "#945e80"
      }
    },
    {
      "scope": [
        "entity.name",
        "entity.static",
        "entity.name.class.static.function",
        "entity.name.function",
        "entity.name.class",
        "entity.name.type"
      ],
      "settings": {
        "foreground": "#b47109"
      }
    },
    {
      "name": "Function declarations",
      "scope": ["entity.function", "entity.name.function.static"],
      "settings": {
        "foreground": "#4c7a5d"
      }
    },
    {
      "scope": "entity.name.function.function-call",
      "settings": {
        "foreground": "#4c7a5d"
      }
    },
    {
      "scope": "support.function.builtin",
      "settings": {
        "foreground": "#c35e0a"
      }
    },
    {
      "scope": [
        "entity.name.method",
        "entity.name.method.function-call",
        "entity.name.static.function-call"
      ],
      "settings": {
        "foreground": "#4c7a5d"
      }
    },
    {
      "scope": "brace",
      "settings": {
        "foreground": "#73503c"
      }
    },
    {
      "name": "variables",
      "scope": [
        "meta.parameter.type.variable",
        "variable.parameter",
        "variable.name",
        "variable.other",
        "variable",
        "string.constant.other.placeholder"
      ],
      "settings": {
        "foreground": "#45707a"
      }
    },
    {
      "scope": "prototype",
      "settings": {
        "foreground": "#945e80"
      }
    },
    {
      "scope": ["punctuation"],
      "settings": {
        "foreground": "#8c6249"
      }
    },
    {
      "scope": "punctuation.quoted",
      "settings": {
        "foreground": "#654735"
      }
    },
    {
      "scope": "punctuation.quasi",
      "settings": {
        "foreground": "#c14a4a"
      }
    },
    {
      "name": "URL",
      "scope": ["*url*", "*link*", "*uri*"],
      "settings": {
        "fontStyle": "underline"
      }
    },
    // ----------------------------------------
    //            LANGUAGE SPECIFIC
    // ----------------------------------------
    // PYTHON ----------------------------------------
    {
      "name": "Python function",
      "scope": ["meta.function.python", "entity.name.function.python"],
      "settings": {
        "foreground": "#4c7a5d"
      }
    },
    {
      "name": "Python Function and Class definition keywords",
      "scope": [
        "storage.type.function.python",
        "storage.modifier.declaration",
        "storage.type.class.python",
        "storage.type.string.python"
      ],
      "settings": {
        "foreground": "#c14a4a"
      }
    },
    {
      "name": "Async keyword",
      "scope": ["storage.type.function.async.python"],
      "settings": {
        "foreground": "#c14a4a"
      }
    },
    {
      "name": "Python Function Call",
      "scope": "meta.function-call.generic",
      "settings": {
        "foreground": "#45707a"
      }
    },
    {
      "name": "Python Function Arguments",
      "scope": "meta.function-call.arguments",
      "settings": {
        "foreground": "#73503c"
      }
    },
    {
      "name": "Python Function decorator",
      "scope": "entity.name.function.decorator",
      "settings": {
        "foreground": "#b47109",
        "fontStyle": "bold"
      }
    },
    {
      "name": "Python ALL CAPS",
      "scope": "constant.other.caps",
      "settings": {
        "fontStyle": "bold"
      }
    },
    // SHELL ----------------------------------------
    {
      "scope": "keyword.operator.logical",
      "settings": {
        "foreground": "#c14a4a"
      }
    },
    {
      "scope": "punctuation.definition.logical-expression",
      "settings": {
        "foreground": "#c35e0a"
      }
    },
    {
      "scope": [
        "string.interpolated.dollar.shell",
        "string.interpolated.backtick.shell"
      ],
      "settings": {
        "foreground": "#4c7a5d"
      }
    },
    // C C++ ----------------------------------------
    {
      "scope": "keyword.control.directive",
      "settings": {
        "foreground": "#4c7a5d"
      }
    },
    {
      "scope": "support.function.C99",
      "settings": {
        "foreground": "#b47109"
      }
    },
    // C# ----------------------------------------
    {
      "name": "C# functions & namespace",
      "scope": [
        "meta.function.cs",
        "entity.name.function.cs",
        "entity.name.type.namespace.cs"
      ],
      "settings": {
        "foreground": "#6c782e"
      }
    },
    {
      "name": "C# Variables",
      "scope": [
        "keyword.other.using.cs",
        "entity.name.variable.field.cs",
        "entity.name.variable.local.cs",
        "variable.other.readwrite.cs"
      ],
      "settings": {
        "foreground": "#4c7a5d"
      }
    },
    {
      "name": "C# This",
      "scope": ["keyword.other.this.cs", "keyword.other.base.cs"],
      "settings": {
        "foreground": "#945e80"
      }
    },
    // MAKEFILE ----------------------------------------
    {
      "scope": "meta.scope.prerequisites",
      "settings": {
        "foreground": "#b47109"
      }
    },
    {
      "scope": "entity.name.function.target",
      "settings": {
        "foreground": "#6c782e",
        "fontStyle": "bold"
      }
    },
    // JAVA ----------------------------------------
    {
      "name": "coloring of the Java import and package identifiers",
      "scope": [
        "storage.modifier.import.java",
        "storage.modifier.package.java"
      ],
      "settings": {
        "foreground": "#805942"
      }
    },
    {
      "scope": ["keyword.other.import.java", "keyword.other.package.java"],
      "settings": {
        "foreground": "#4c7a5d"
      }
    },
    {
      "scope": "storage.type.java",
      "settings": {
        "foreground": "#b47109"
      }
    },
    {
      "scope": "storage.type.annotation",
      "settings": {
        "foreground": "#45707a",
        "fontStyle": "bold"
      }
    },
    {
      "scope": "keyword.other.documentation.javadoc",
      "settings": {
        "foreground": "#4c7a5d"
      }
    },
    {
      "scope": "comment.block.javadoc variable.parameter.java",
      "settings": {
        "foreground": "#6c782e",
        "fontStyle": "bold"
      }
    },
    {
      "scope": [
        "source.java variable.other.object",
        "source.java variable.other.definition.java"
      ],
      "settings": {
        "foreground": "#654735"
      }
    },
    // LISP ----------------------------------------
    {
      "name": "Lisp optional function parameters",
      "scope": "meta.function-parameters.lisp",
      "settings": {
        "foreground": "#b47109"
      }
    },
    // MARKUP ----------------------------------------
    {
      "scope": "markup.underline",
      "settings": {
        "fontStyle": "underline"
      }
    },
    {
      "scope": "string.other.link.title.markdown",
      "settings": {
        "foreground": "#928374",
        "fontStyle": "underline"
      }
    },
    {
      "scope": "markup.underline.link",
      "settings": {
        "foreground": "#945e80"
      }
    },
    {
      "scope": "markup.bold",
      "settings": {
        "fontStyle": "bold",
        "foreground": "#c35e0a"
      }
    },
    {
      "scope": "markup.heading",
      "settings": {
        "fontStyle": "bold",
        "foreground": "#c35e0a"
      }
    },
    {
      "scope": "markup.italic",
      "settings": {
        "fontStyle": "italic"
      }
    },
    {
      "scope": "markup.inserted",
      "settings": {
        "foreground": "#6c782e"
      }
    },
    {
      "scope": "markup.deleted",
      "settings": {
        "foreground": "#c35e0a"
      }
    },
    {
      "scope": "markup.changed",
      "settings": {
        "foreground": "#c35e0a"
      }
    },
    {
      "scope": "markup.punctuation.quote.beginning",
      "settings": {
        "foreground": "#6c782e"
      }
    },
    {
      "scope": "markup.punctuation.list.beginning",
      "settings": {
        "foreground": "#45707a"
      }
    },
    {
      "scope": ["markup.inline.raw", "markup.fenced_code.block"],
      "settings": {
        "foreground": "#4c7a5d"
      }
    },
    // JSON ----------------------------------------
    {
      "scope": "string.quoted.double.json",
      "settings": {
        "foreground": "#45707a"
      }
    },
    {
      "name": "JSON Level 0",
